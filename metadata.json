{
  "name": "novakv-rclocal",
  "version": "1.0.0",
  "author": "novakv",
  "summary": "Manages /etc/rc.d/rc.local",
  "license": "Apache License, Version 2.0",
  "source": "https://github.com/novakv/puppet-rclocal.git",
  "project_page": "https://github.com/novakv/puppet-rclocal",
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.7.3"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04",
        "16.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    }
  ],
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 4.5.0"}
  ]
}
